<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.CcairbagCarBrandsMapper">
    
    <resultMap type="CcairbagCarBrands" id="CcairbagCarBrandsResult">
        <result property="id"    column="id"    />
        <result property="make"    column="make"    />
        <result property="model"    column="model"    />
        <result property="year"    column="year"    />
    </resultMap>

    <sql id="selectCcairbagCarBrandsVo">
        select id, make, model, year from ccairbag_car_brands
    </sql>

    <select id="selectCcairbagCarBrandsList" parameterType="CcairbagCarBrands" resultMap="CcairbagCarBrandsResult">
        <include refid="selectCcairbagCarBrandsVo"/>
        <where>
            deleted = 0
            <if test="make != null  and make != ''"> and make = #{make}</if>
            <if test="model != null  and model != ''"> and model = #{model}</if>
            <if test="year != null  and year != ''"> and year = #{year}</if>
        </where>
    </select>
    
    <select id="selectCcairbagCarBrandsById" parameterType="String" resultMap="CcairbagCarBrandsResult">
        <include refid="selectCcairbagCarBrandsVo"/>
        where deleted = 0 and id = #{id}
    </select>

    <insert id="insertCcairbagCarBrands" parameterType="CcairbagCarBrands">
        insert into ccairbag_car_brands
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="make != null">make,</if>
            <if test="model != null">model,</if>
            <if test="year != null">year,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="make != null">#{make},</if>
            <if test="model != null">#{model},</if>
            <if test="year != null">#{year},</if>
         </trim>
    </insert>

    <update id="updateCcairbagCarBrands" parameterType="CcairbagCarBrands">
        update ccairbag_car_brands
        <trim prefix="SET" suffixOverrides=",">
            <if test="make != null">make = #{make},</if>
            <if test="model != null">model = #{model},</if>
            <if test="year != null">year = #{year},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCcairbagCarBrandsById" parameterType="String">
        update ccairbag_car_brands set deleted = 1 where id = #{id}
    </delete>

    <delete id="deleteCcairbagCarBrandsByIds" parameterType="String">
        update ccairbag_car_brands set deleted = 1 where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>

    </delete>
</mapper>